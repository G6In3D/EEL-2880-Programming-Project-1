#include <stdio.h>

int main(void) {
  int x; //assign counter to x
  int y = -12; //assign the starting integer value to cube
  
  puts("These are the cube numbers from -12 to 12\n" ); //header
  
  puts("Values\t\t\Cube\t\t\n"); //create table
  
  //loop a total of 24 times to calculate cubes from all even and odd numbers for 12 to -12
  
  for( x = 0; x <= 24; ++x ) {
    printf("%d\t\t%d\t\t\n", y, (y * y * y) ); //print the cube
    
    y++; increment by 1
  } //end outer for loop
} //end main
